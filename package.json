{
  "name": "bestil",
  "version": "0.1.0",
  "private": true,
  "description": " A simple and elegant book finder web app using react.",
  "homepage": "https://murshidazher.github.io/bestil",
  "dependencies": {
    "fetch-mock": "^7.3.9",
    "husky": "^3.0.8",
    "node-fetch": "^2.6.7",
    "pretty-quick": "^1.11.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-scripts": "5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "export NODE_ENV=development && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "gh-pages": "^2.1.1",
    "prettier": "1.18.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3"
  }
}
